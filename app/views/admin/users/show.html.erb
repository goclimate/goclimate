<%= render 'admin/shared/notice_banner' %>

<h1>User</h1>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @user.id %></dd>
  <dt>Email:</dt>
  <dd><%= @user.email %></dd>
  <dt>Admin:</dt>
  <dd><%= @user.admin %></dd>
  
  <dt>Updated:</dt>
  <dd><%= time_tag(@user.updated_at) %></dd>
</dl>

<hr>

<%= link_to 'Edit', edit_admin_user_path(@user), class: 'btn btn-default' %>
<%= link_to 'Back', admin_users_path, class: 'btn btn-link' %>
